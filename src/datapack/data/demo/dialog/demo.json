			},
			"tooltip": {
				"translate": "essentials.demo.dialog.actions.remove_fake_players.tooltip",
				"fallback": "Does not reset %s counter, that would require a manual reset.",
				"with":[
					{
						"translate": "essentials.scoreboard.playerid",
						"fallback": "playerid"
					}
				]
			},
			"action": {
				"type": "minecraft:run_command",
				"command": "function demo:clean_up"
			}
		},
		{
			"label": {
				"translate": "essentials.demo.dialog.actions.fake_requests.label",
				"fallback": "Enter fake requests"
			},
			"tooltip": {
				"translate": "essentials.demo.dialog.actions.fake_requests.tooltip",
				"fallback": "These fake requests are only for %s 1",
				"with":[
					{
						"translate": "essentials.scoreboard.playerid",
						"fallback": "Player ID"
					}
				]
			},
			"width": 300,
			"action": {
				"type": "minecraft:run_command",
				"command": "function demo:demo_requests"
			}
		}
