	"pause": false,
	"after_action": "close",
	"actions": [
		{
			"label": {
				"translate": "essentials.demo.dialog.actions.spawn_fake_players.label",
				"fallback": "Spawn fake players"
			},
			"action": {
				"type": "minecraft:run_command",
				"command": "function demo:spawn_nonplayers"
			}
		},
		{
			"label": {
				"translate": "essentials.demo.dialog.actions.remove_fake_players.label",
