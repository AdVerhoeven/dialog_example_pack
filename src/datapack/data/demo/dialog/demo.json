{
	"type": "minecraft:multi_action",
	"title": {
		"translate": "essentials.demo.dialog.title",
		"fallback": "Demo functions"
	},
	"external_title": {
		"translate": "essentials.demo.dialog.external_title",
		"fallback": "Demo"
	},
	"body": {
		"type": "minecraft:plain_message",
		"contents": {
			"translate": "essentials.demo.dialog.body",
			"fallback": "These actions require you to be server operator"
		}
	},
	"inputs": [],
