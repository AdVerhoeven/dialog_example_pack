{
    "data_pack": {
        "load": [
            "src/datapack"
        ],
        "name": "essentials"
    },
    "resource_pack": {
        "load": [
            "src/resourcepack"
        ],
        "name": "essentials_lang"
    },
    "pipeline": [
        "plugins.custom.main",
        "mecha",
        "plugins.lang_resources.validate_alt_languages",
        "plugins.multi_lang.collect_default_lang",
        "plugins.multi_lang.check_functions"
    ],
    "id": "tp",
    "name": "essentials",
    "description": "Dialogs for teleport requests",
    "version": "1.0.0a",
    "require": [
        "bolt",
        "bolt_expressions"
    ],
    "output": "build",
    "meta": {
        "bolt": {
            "entrypoint": [
                "tp:main"
            ]
        },
        "def_lang": "en_us"
    }
}