{
    "data_pack": {
        "load": [
            "src/datapack"
        ],
        "name": "essentials"
    },
    "resource_pack": {
        "load": [
            "src/resourcepack"
        ],
        "name": "essentials_lang"
    },
    "pipeline": [
        "plugins.custom.main",
        "mecha",
        "plugins.validate_text_components.check_functions",
        "plugins.validate_text_components.validate_alt_languages"
    ],
    "id": "tp",
    "name": "essentials",
    "description": "Dialogs for teleport requests",
    "version": "1.0.0a",
    "require": [
        "bolt",
        "bolt_expressions"
    ],
    "output": "build",
    "meta": {
        "bolt": {
            "entrypoint": [
                "tp:main"
            ]
        }
    }
}