{
    "type": "notice",
    "title": "Help",
    "external_title": "Help",
    "pause": false,
    "body": [
        {
            "type": "minecraft:plain_message",
            "contents": [
                {
                    "text": "You can test the singleoption player selection dialog using:\n"
                },
                {
                    "text": "/function essentials:player/start\n",
                    "click_event": {
                        "action": "run_command",
                        "command": "/function essentials:dialog/player/singleoption/start"
                    }
                },
                {
                    "text": "This dialog can also be accessed using the trigger command\n"
                },
                {
                    "text": "/trigger open_dialog 1\n",
                    "click_event": {
                        "action": "run_command",
                        "command": "/trigger open_dialog set 1"
                    }
                }
            ]
        },
        {
